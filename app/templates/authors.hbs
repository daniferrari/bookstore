<h1>List all authors</h1>

{{#link-to 'authors.new' class="btn btn-primary"}}Add new author{{/link-to}}


<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
  {{#each model as |author|}}
    <tr>
      <td>{{author.name}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>


{{outlet}}
